        <Button x:Name="btnShowLog" Content="Log" HorizontalAlignment="Left" Margin="211.5,488,0,0" VerticalAlignment="Top" Width="27" Height="23" Click="btnShowLog_Click" Grid.Column="1"/>
        <Label x:Name="lblWarning" Content="Warning:" HorizontalAlignment="Left" Margin="191,362,0,0" VerticalAlignment="Top" Width="585" FontSize="16" Visibility="Hidden" Grid.ColumnSpan="2"/>
