        <ComboBox x:Name="cmbProduct" HorizontalAlignment="Left" Margin="540,476,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cmbProduct_SelectionChanged" Grid.ColumnSpan="2"/>
        <Button x:Name="btnSellAtNow" Content="Sell NOW" HorizontalAlignment="Left" Margin="102.5,435,0,0" VerticalAlignment="Top" Width="136" Click="btnSellAtNow_Click" Grid.Column="1"/>
        <Label x:Name="lblStatus" Content="Status" HorizontalAlignment="Left" Margin="235,45,0,0" VerticalAlignment="Top"/>
        <Label Content="Next action:" HorizontalAlignment="Left" Margin="505,556,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblNextAction" Content="NOT SET" HorizontalAlignment="Left" Margin="583,556,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Label x:Name="lblSmaSmall" Content="(small sma)" HorizontalAlignment="Left" Margin="375,105,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnUpdateSmallSma" Content="Update small sma" Grid.Column="1" HorizontalAlignment="Left" Margin="102.5,96,0,0" VerticalAlignment="Top" Width="135" Click="btnUpdateSmallSma_Click"/>
        <TextBox x:Name="txtSmallSmaInterval" Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="26.5,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40"/>
        <TextBox x:Name="txtSmallSmaSlices" Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="26.5,115,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40"/>
        <Label Content="min" HorizontalAlignment="Left" Margin="592,91,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Label Content="slices" HorizontalAlignment="Left" Margin="592,114,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Label x:Name="lblSmallSd" Content="(small sma sd)" HorizontalAlignment="Left" Margin="465,105,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.228,-0.141"/>

    </Grid>
